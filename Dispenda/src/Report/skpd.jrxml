	<parameter name="no_kohir" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     *,
     tb_skpd.`no_kohir` AS tb_skpd_no_kohir,
     tb_skpd.`npwpd_3` AS tb_skpd_npwpd_3,
     tb_skpd.`nama_badan` AS tb_skpd_nama_badan,
