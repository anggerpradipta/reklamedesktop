     tb_skpd.`jumlah_bayar` AS tb_skpd_jumlah_bayar,
     tb_skpd.`bunga` AS tb_skpd_bunga,
     tb_skpd.`kenaikan` AS tb_skpd_kenaikan,
     tb_skpd.`total` AS tb_skpd_total
FROM
     `tb_skpd` tb_skpd
